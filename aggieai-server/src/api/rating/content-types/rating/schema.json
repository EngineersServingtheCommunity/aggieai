{
  "kind": "collectionType",
  "collectionName": "ratings",
  "info": {
    "singularName": "rating",
    "pluralName": "ratings",
    "displayName": "rating",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "author": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "review": {
      "type": "text",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true,
      "default": 1,
      "max": 10,
      "min": 1
    },
    "professor": {
      "type": "string",
      "required": true
    },
    "votes": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "created": {
      "type": "date",
      "required": true
    },
    "grade": {
      "type": "enumeration",
      "enum": [
        "A",
        "B",
        "C",
        "D"
      ],
      "required": true,
      "default": "A"
    },
    "semester": {
      "type": "date"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "rating"
    },
    "class": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::class.class",
      "inversedBy": "ratings"
    }
  }
}
